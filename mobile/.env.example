EXPO_PUBLIC_SERVER_URL=http://localhost:4000
# Optional: Google login for mobile
EXPO_PUBLIC_GOOGLE_CLIENT_ID=
# Required for web push (browser) support
EXPO_PUBLIC_VAPID_PUBLIC_KEY=
# Path to the service worker that handles web push (relative to web root)
EXPO_PUBLIC_NOTIFICATION_SW_PATH=expo-service-worker.js
# Required for Expo push tokens (set your EAS project ID)
EXPO_PUBLIC_EAS_PROJECT_ID=
# Optional: Android applicationId override (defaults to android.package in config)
EXPO_PUBLIC_ANDROID_APPLICATION_ID=com.odsa.mobile
